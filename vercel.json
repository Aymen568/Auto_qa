{
    "version": 2,
    "builds": [
      {
        "src": "Backend/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "Frontend/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "Backend/app.py"
      },
      {
        "src": "/(.*)",
        "dest": "Frontend/$1"
      }
    ]
  }